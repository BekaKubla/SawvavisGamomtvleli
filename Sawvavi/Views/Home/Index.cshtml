@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>mical</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- bootstrap css -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <!-- style css -->
    <link href="~/css/style.css" rel="stylesheet" />
    <!-- Responsive-->
    <link href="~/css/responsive.css" rel="stylesheet" />
    <!-- fevicon -->
    <link rel="icon" href="~/images/fevicon.png" type="image/gif" />
    <!-- Scrollbar Custom CSS -->
    <link href="~/css/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<!-- body -->
<body class="main-layout">
    <!-- wedo  section -->
    <div class="wedo ">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="titlepage">
                        <h2>რას გთავაზობთ?</h2>
                        <p>ქალაქებს შორის მანძილით საწვავის გამოთვლა </p>
                        <p>საწვავის ფასები,რომელიც ყოველ ერთ საათში ერთხელ ავტომატურად ახლდება </p>
                        <p>ავტომობილის განბაჟების გამოთვლა</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10 offset-md-1">
                    <div class="row">
                        <div class="col-md-6 margin_bottom">
                            <div class="work">
                                <figure><img src="~/images/map1.1.jpg" alt="#" /></figure>
                            </div>
                            <div class="work_text">
                                <a asp-controller="Fuel" asp-action="MapAndGasoline"><h3>რუკა<br><span class="blu">გამოთვალე საწვავის რაოდენობა</span></h3></a>
                            </div>
                        </div>
                        <div class="col-md-6 margin_bottom">
                            <div class="work">
                                <figure><img src="~/images/gasoline1.1.jpg" alt="#" /></figure>
                            </div>
                            <div class="work_text">
                                <a asp-controller="Scraping" asp-action="FuelPrice"><h3>საწვავი<br><span class="blu">გაიგე საწვავის ფასები</span></h3></a>
                            </div>
                        </div>
                        <div class="col-md-6 margin_bottom">
                            <div class="work">
                                <figure><img src="~/images/car1.1.jpg" alt="#" /></figure>
                            </div>
                            <div class="work_text">
                                <a asp-controller="home" asp-action="Ganbajeba"> <h3>განბაჟება<br> <span class="blu">გამოთვალე ავტომობილის განბაჟება</span></h3></a>
                            </div>
                        </div>
                        <div class="col-md-6 margin_bottom">
                            <div class="work">
                                <figure><img src="~/images/contact1.2.jpg" alt="#" /></figure>
                            </div>
                            <div class="work_text">
                                <a asp-controller="Contact" asp-action="Index"><h3>კონტაქტი<br><span class="blu">დაგვიტოვე შეტყობინება</span></h3></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end wedo  section -->
    <!--  footer -->
    <footer id="contact">
        <div class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-4">
                        <div class="titlepage">
                            <h2>კონტაქტი</h2>
                        </div>
                    </div>
                    <div class="col-md-8">
                        <ul class="location_icon">
                            <li><i class="fa fa-volume-control-phone" aria-hidden="true"></i> +995 579025090</li>
                            <li><i class="fa fa-envelope" aria-hidden="true"></i> InfoMandzili@Gmail.Com</li>
                            <li><a href="https://www.facebook.com/mandzili.ge" target="_blank"><i class="fa fa-facebook" aria-hidden="true"></i></a>Mandzili.GE</li>
                        </ul>
                    </div>
                    <div class="col-md-6">
                        @await Component.InvokeAsync("Contact")
                    </div>
                </div>
            </div>
            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <p>Copyright 2021 All Right Reserved</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- end footer -->
    <!-- Javascript files-->
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/jquery-3.0.0.min.js"></script>
    <!-- sidebar -->
    <script src="~/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/js/custom.js"></script>
</body>
</html>